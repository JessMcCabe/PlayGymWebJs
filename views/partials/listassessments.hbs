   {{#each assessment}}
<table class="ui table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Weight</th>
      <th>Chest</th>
      <th>Thigh</th>
      <th>Upper Arm</th>
      <th>Waist</th>
      <th>Hips</th>
      <th>Trend</th>
      <th>Comments</th>
        <th></th>
    </tr>
  </thead>
 <tbody>
      <tr>
          <td>
              {{date}}
          </td>
        <td>
          {{weight}}
        </td>
        <td>
          {{chest}}
        </td>
         <td>
          {{thigh}}
        </td>
        <td>
          {{upperArm}}
        </td>
         <td>
          {{waist}}
        </td>
        <td>
          {{hips}}
        </td>
        <td>
            <i><a class="ui {{trend}} tag label"></a></i>
        </td>
        <td>
           {{comment}}
        </td>
          <td>
          <a href="/dashboard/deleteAssessment/{{id}}" class="ui icon button">
              <i class="icon trash"></i>
          </a>
          </td>
      </tr>
    {{/each}}
  </tbody>
</table>
